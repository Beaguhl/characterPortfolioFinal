<h1 class="title text-center">
    Update character
</h1>
<div class="parchment">
    {{>header}}
    
<form action="/character/{{characterId}}/update" method="POST">
    <label>Name</label>
    <input type="text" name="name" value="{{warhammerCharacter.name}}">
    <label>Race</label>
    <input type="text" name="race" value="{{warhammerCharacter.race}}">
    <label>Stats</label>
    <input type="text" name="stats" value="{{warhammerCharacter.stats}}">
    <input type="submit" name="Update" value="update">
</form>
</div>